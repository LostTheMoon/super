#include<stdio.h>
int main()
{
	char arr1[] = "abcdef";
	char arr2[] = "abcdef";
	const char* p1 = "abcdef";
	const char* p2 = "abcdef";
	//常量字符串 若两个均为常量字符串 系统不会在生成一个空间去在存一个相同的常量字符串
	if (arr1 == arr2)
	{
		printf("arr1=arr2\n");
	}
	else
	{
		printf("arr1!=arr2\n");
	}
	//
	if (p1 == p2)
	{
		printf("p1=p2\n");
	}
	else
	{
		printf("p1!=p2\n");
	}
	return 0;
}
