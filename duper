//不生成临时变量，使两数交换
#include<stdio.h>
{
  int a = 3 ;
  int b = 5 ;
  printf("before:a=%d,b=%d\n", a , b );
  //加减法-可能会溢出
  a = a + b;
  b = a - b;
  a = a - b;
  printf("after :a=%d,b=%d\n", a , b );
  int x = 3;
  int y = 5;
  printf("before:a=%d,b=%d\n", x , y );
  //异或法
  x = x^y;
  y = x^y;
  x = x^y;
  printf("after :a=%d,b=%d\n", x , y );
  return 0 ;
}
